<?xml version="1.0" encoding="utf-8"?>
<Patch
<!-- remove VFE recipes from Chirurgie Patch -->
<!-- Settlers -->
  <Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Factions Expanded - Settlers</li> <!-- OskarPotocki.VanillaFactionsExpanded.SettlersModule --> 
		</mods>
		<match Class="PatchOperationSequence">
			<success>Normal</success>
			<operations>		
			        <li Class="PatchOperationRemove">
          				<xpath>Defs/RecipeDef[defName="settlermed"]/recipeUsers</xpath>
        			</li>
       </operations>
    </match>
	</Operation>
</Patch>
